<body>
    <!-- #header -->
    <% include header.html %>
        <!-- start banner Area -->
        <section class="banner-area_join relative">
            <div class="overlay overlay-bg"></div>
            <div class="container">
                <div class="row fullscreen align-items-center justify-content-between">
                    <div class="col-lg-5 col-md-6 banner-left">
                        <h1 class="text-white">로그인</h1>
                        <p class="text-white">
                            회원이 되면 더 많을 기능을 이용할 수 있습니다.
                        </p>
                        <br>
                        <button class="primary-btn" id="join_button" onclick="location.href='/joinForm'">회원가입</button>
                    </div>
                    <div class="col-lg-5 col-md-6 banner-right">
                        <ul class="nav nav-tabs" id="myTab" role="tablist">
                            <li class="nav-item">
                                <a class="nav-link active" href="#login_customer" id="customer-tab" data-toggle="tab" role="tab" aria-controls="customer" aria-selected="true">소비자</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#login_seller" id="seller-tab" data-toggle="tab" role="tab" aria-controls="seller" aria-selected="false">판매자</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#login_admin" id="seller-tab" data-toggle="tab" role="tab" aria-controls="admin" aria-selected="false">관리자</a>
                            </li>
                        </ul>
                        <div class="tab-content" id="myTabContent">
                            <!-- 소비자 로그인창 -->
                            <div class="tab-pane fade show active" id="login_customer" role="tabpanel" aria-labelledby="login_customer">
                                <form class="form-wrap" method="post" onsubmit="return onWriteSubmit()" action="/index/login">
                                    <div class="form-group col-md-12">
                                        <input type="text" class="form-control" id="user_id" name="user_id" placeholder="ID" required/>
                                    </div>
                                    <div class="form-group col-md-12">
                                        <input type="password" class="form-control" id="user_passwd" name="user_passwd" placeholder="Password" required/>
                                    </div>
                                    <div class="form-check form-check-inline" style="display: none">
                                        <input class="form-check-input" type="radio" id="user_type" name="user_type" value="customer" checked>
                                    </div>
                                    <button class="primary-btn" type="submit" id="customer_button">Sign in</button>
                                </form>
                            </div>
                            <!-- 판매자 로그인창 -->
                            <div class="tab-pane fade" id="login_seller" role="tabpanel" aria-labelledby="login_customer">
                                <form class="form-wrap" method="post" onsubmit="return onWriteSubmit()" action="/index/login">
                                    <div class="form-group col-md-12">
                                        <input type="text" class="form-control" id="user_id" name="user_id" placeholder="ID" required/>
                                    </div>
                                    <div class="form-group col-md-12">
                                        <input type="password" class="form-control" id="user_passwd" name="user_passwd" placeholder="Password" required/>
                                    </div>
                                    <div class="form-check form-check-inline" style="display: none">
                                        <input class="form-check-input" type="radio" id="user_type" name="user_type" value="seller" checked>
                                    </div>
                                    <button class="primary-btn" type="submit" id="seller_button">Sign in</button>
                                </form>
                            </div>
                            <!-- 관리자 로그인창 -->
                            <div class="tab-pane fade" id="login_admin" role="tabpanel" aria-labelledby="login_admin">
                                <form class="form-wrap" method="post" onsubmit="return onWriteSubmit()" action="/index/login">
                                    <div class="form-group col-md-12">
                                        <input type="text" class="form-control" id="user_id" name="user_id" placeholder="ID" required/>
                                    </div>
                                    <div class="form-group col-md-12">
                                        <input type="password" class="form-control" id="user_passwd" name="user_passwd" placeholder="Password" required/>
                                    </div>
                                    <div class="form-check form-check-inline" style="display: none">
                                        <input class="form-check-input" type="radio" id="user_type" name="user_type" value="admin" checked>
                                    </div>
                                    <button class="primary-btn" type="submit" id="seller_button">Sign in</button>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- End banner Area -->
        <!-- start footer Area -->
        <footer class="footer-area section-gap">
